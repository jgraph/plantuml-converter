@startuml
title Concurrent Regions

state ConcurrentH {
    [*] --> A1
    A1 --> A2
    A2 --> [*]
    --
    [*] --> B1
    B1 --> B2
    B2 --> [*]
}

state ConcurrentV {
    [*] --> C1
    C1 --> C2
    C2 --> [*]
    ||
    [*] --> D1
    D1 --> D2
    D2 --> [*]
}

state ThreeRegions {
    [*] --> E1
    E1 --> [*]
    --
    [*] --> F1
    F1 --> [*]
    --
    [*] --> G1
    G1 --> [*]
}

[*] --> ConcurrentH
ConcurrentH --> ConcurrentV
ConcurrentV --> ThreeRegions
ThreeRegions --> [*]

@enduml
